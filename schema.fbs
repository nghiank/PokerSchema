//Schema for Netty Chat Server
namespace schema;

union Data {CredentialToken, ReconnectKey, GameCommand}

enum Command : byte { JoinRoom = 1, QuitRoom }

table GameCommand {
  type: Command; 
}

table ReconnectKey {
  key:string (required);
}

table CredentialToken {
  token:string (required);
}

table Message {
  data:Data;
}

root_type Message;
